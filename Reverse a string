import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner in=new Scanner(System.in);
	    System.out.print("Enter a string:");
	    String sc=in.nextLine();
	    String rev="";
	    for(int i=sc.length()-1;i>=0;i--)
	    {
	        rev=rev+sc.charAt(i);
	    }
	    System.out.println(rev);
	    
	}

}
