import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner in=new Scanner(System.in);
	    System.out.print("Enter a number:");
	    int n=in.nextInt();
	    boolean ans=isPrime(n);
	    System.out.println(ans);
	    
	    
	}
	static boolean isPrime(int n)
	{
	    for(int i=2;i<=n;i++)
	    {
	        if(n%i!=0)
	        {
	            return true;
	        }
	    }
	    return false;
	}
}
