public class Main
{
	public static void main(String[] args) {
		int arr[]={30,50,20,10,40};
		int temp;
		for(int i=0;i<arr.length;i++)
		{
		    for(int j=i+1;j<arr.length;j++)
		    {
		        if(arr[i]>arr[j])
		        {
		            temp=arr[j];
		            arr[j]=arr[i];
		            arr[i]=temp;
		        }
		    }
		}
		for(int i=0;i<arr.length;i++)
		{
		    System.out.print(arr[i]+" ");
		}
	}
}
